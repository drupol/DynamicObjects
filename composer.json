{
    "name": "drupol/dynamicobjects",
    "description": "Create and manage dynamic properties and methods on a PHP object.",
    "license": "GPL",
    "homepage": "https://github.com/drupol/dynamicobjects",
    "keywords": ["dynamic methods", "dynamic properties"],
    "support": {
        "source": "https://github.com/drupol/dynamicobjects",
        "forum": "https://github.com/drupol/dynamicobjects/issues",
        "irc": "irc://irc.freenode.org/php"
    },
    "authors": [{
        "name": "Pol Dellaiera",
        "email": "pol.dellaiera@protonmail.com",
        "homepage": "http://not-a-number.io",
        "role": "Author"
    }],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "drupol\\DynamicObjects\\": "src/"
        }
    },
    "require-dev": {
        "phpro/grumphp": "^0.12",
        "phpspec/phpspec": "^3.0",
        "squizlabs/php_codesniffer": "^3.0",
        "leanphp/phpspec-code-coverage": "^3.1",
        "scrutinizer/ocular": "^1.4",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "sensiolabs/security-checker": "^4.1",
        "drupol/memoize": "^1.0",
        "fetzi/phpspec-watcher": "^1.0"
    },
    "suggest": {
        "drupol/memoize": "DynamicObjects requires this package to get memoization working."
    },
    "scripts": {
        "phpspec": "./bin/phpspec run",
        "grumphp": "./bin/grumphp run",
        "scrutinizer": "./bin/ocular code-coverage:upload --format=php-clover build/coverage.xml"
    }
}
